  <!--呼び出されるJavaクラスファイルと実行するメソッドを宣言  -->
  
  <!-- GoHomeAction -->
  <action name= "GoHomeAction" class="com.diworksdev.ecsite.action.GoHomeAction" method="execute">
    <result name="success">home.jsp</result>
  </action>
  
  
</package>
</struts>